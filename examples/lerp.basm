jmp main

# a + (b - a) * t 
lerpf:
	dup 3
	dup 3
	dup 1
	minusf
	dup 3
	mulf
	plusf

	# Clean the stack
	swap 2
	drop
	swap 2
	drop
	swap 2
	drop
	ret

main:
	push 10.0
	push 20.0
	push 0.0
	call lerpf
	native 2

	push 10.0
	push 20.0
	push 1.0
	call lerpf
	native 2

	push 10.0
	push 20.0
	push 0.5
	call lerpf
	native 2

	halt