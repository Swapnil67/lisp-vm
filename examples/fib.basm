: Initial Stack
: 100
: 1
: 0

jmp main

main: 
  : First two fibonacci numbers & Counter  
  push 0      			  	: 1st fib number
  push 1				: 2nd fib number
  push 20				: N - amount of iterations
  : The Loop
  loop:
	: Find Fib
	swap 2
  	dup 1

	: Print the number
	dup 0
	native 3

	plusf
	swap 1

	: Check counter condition
	swap 2
	push 1
	minusi 1

	dup 0
	push 0
	eq
	not

	jmp_if loop

halt