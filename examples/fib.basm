%include "./examples/natives.hasm"
%bind N 10

; Initial Stack
; 100
; 1
; 0

%entry main

main: 
  ; First two fibonacci numbers & Counter  
  push 0      	; 1st fib number
  push 1				; 2nd fib number
  push N				; N - amount of iterations
  ; The Loop
  loop:
	; Find Fib
	swap 2
	dup 1

	; Print the number
	dup 0
	call dump_u64

	plusi
	swap 1

	; Check counter condition
	swap 2
	push 1
	minusi 1

	dup 0
	push 0
	eqi
	not

	jmp_if loop

halt